{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "ES2019",
    "lib": ["ES2019", "DOM"],
    "types": ["office-js"],
    "strict": false
  },
  "exclude": [
    "node_modules",
    "dist",
    "src/**/__tests__/**",
    "src/**/*.test.ts",
    "src/**/*.test.tsx",
    "**/*.test.ts",
    "**/*.test.tsx"
  ]
}